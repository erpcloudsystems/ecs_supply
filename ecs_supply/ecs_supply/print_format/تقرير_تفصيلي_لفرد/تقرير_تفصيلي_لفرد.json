45\u062d\u0643\u0645\u0629</th>\r\n                        <th >\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0642\u0631\u0627\u0631</th>\r\n                        <th >\u0645\u0648\u0642\u0641\u0647 \u0645\u0646 \u0627\u0644\u0639\u0645\u0644</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                  {% for license in doc_CTOR.statement_of_the_case %}\r\n                    <tr>\r\n                          <td >{{license.the_case_number or \"-\"}}</td>\r\n                          <td >{{license.the_charge or \"-\"}}</td>\r\n                          <td >{{license.public_prosecution_decision or \"-\"}}</td>\r\n                          <td >{{license.date_of_the_decision or \"-\"}}</td>\r\n                          <td >{{license.the_courts_decision or \"-\"}}</td>\r\n                          <td >{{license.date_of_the_decision2 or \"-\"}}</td>\r\n                          <td >{{license.his_attitude_to_work or \"-\"}}</td>\r\n                    </tr>\r\n                    {% endfor %}\r\n                </tbody>\r\n            </table>\r\n            </div>\r\n            <div style=\"height:2em;\"></div>\r\n        \r\n            <hr>\r\n            {%endif%}\r\n        \r\n  {%endif%}\r\n  <!-- \u0628\u064a\u0627\u0646 \u0645\u062c\u0627\u0644\u0633 \u0627\u0644\u062a\u0623\u062f\u064a\u0628 -->\r\n  {% if frappe.db.exists(\"Policemen Punishment\", {\"policeman_name\": doc.name}) %}\r\n      {% set doc_pp = frappe.get_doc(\"Policemen Punishment\", {\"policeman_name\": doc.name}) %}\r\n  \r\n        <br>  \r\n        <br>\r\n            <div style=\"height:2em;\"></div>\r\n              \r\n            {% if doc_pp.data %}\r\n        \r\n            <div style=\"display: flex; justify-content:center;\">\r\n              <div style=\" display: flex;flex-direction: column; justify-content:center; align-items: center;margin-top:-1em\">\r\n            \r\n                <div>\r\n                    <h3 style=\"border:2px solid black; background-color:lightgray;font-family:Cairo; margin: 0px 20px; width:200px;text-align:center; padding-left: 0%;\">\u0628\u064a\u0627\u0646 \u0645\u062c\u0627\u0644\u0633 \u0627\u0644\u062a\u0623\u062f\u064a\u0628</h3>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <hr>\r\n          <div  class=\"table1\">\r\n            <table>\r\n                <thead>\r\n                    <tr>\r\n                        <th >\u0628\u064a\u0627\u0646 \u0627\u0644\u0645\u062e\u0627\u0644\u0641\u0629</th>\r\n                        <th >\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0645\u062e\u0627\u0644\u0641\u0629</th>\r\n                        <th >\u0631\u0642\u0645 \u0642\u0631\u0627\u0631 \u0627\u0644\u0627\u062d\u0627\u0644\u0629</th>\r\n                        <th>\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0627\u062d\u0627\u0644\u0629</th>\r\n                        <th>\u0631\u0642\u0645 \u0627\u0644\u062f\u0639\u0648\u0629</th>\r\n                        <th>\u0642\u0631\u0627\u0631 \u0645\u062c\u0644\u0633 \u0627\u0644\u062a\u0627\u062f\u064a\u0628 \u0627\u0644\u0646\u0647\u0627\u0626\u064a</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                  {% for license in doc_pp.data %}\r\n                    <tr>\r\n                          <td >{{license.punishment_type or \"-\"}}</td>\r\n                          <td >{{license.punishment_date or \"-\"}}</td>\r\n                          <td >{{license.referral_decision_number or \"-\"}}</td>\r\n                          <td >{{license.referral_date or \"-\"}}</td>\r\n                          <td >{{license.call_number or \"-\"}}</td>\r\n                          <td >{{license.disciplinary_board_decision or \"-\"}}</td>\r\n                    </tr>\r\n                    {% endfor %}\r\n                </tbody>\r\n            </table>\r\n            </div>\r\n            <div style=\"height:2em;\"></div